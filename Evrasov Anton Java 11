import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Введите два целых числа:");
        int a = scanner.nextInt();
        int b = scanner.nextInt();
        if (a > b) {
            System.out.println("a > b");
        } else if (a < b) {
            System.out.println("a < b");
        } else {
            System.out.println("a = b");
        }
        a += b;
        a -= b;
        a /= b;
        a *= b;
        System.out.println("\nРезультат сложения: " + a);
        System.out.println("\nРезультат вычитания: " + a);
        System.out.println("\nРезультат деления: " + a);
        System.out.println("\nРезультат умножения: " + a);
    }
}
