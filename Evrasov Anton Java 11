public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Введите два целых числа:");
        int a = scanner.nextInt();
        int b = scanner.nextInt();
        if (a > b) {
            System.out.println("a > b");
        } else if (a < b) {
            System.out.println("a < b");
        } else {
            System.out.println("a = b");
        }
        int summ = a + b;
        int subtract = a - b;
        int division = a / b;
        int multiplicat = a * b;
        System.out.println("\nРезультат сложения: " +  summ);
        System.out.println("\nРезультат вычитания: " + subtract);
        System.out.println("\nРезультат деления: " + division);
        System.out.println("\nРезультат умножения: " + multiplicat);
    }
}
